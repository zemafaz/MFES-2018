class Exhibitor is subclass of Entity
	instance variables
		private static nextId : nat1 := 1;
	
	operations
		--Constructor
		public Exhibitor : seq of char ==> Exhibitor
			Exhibitor(n) == (
				id := nextId;
				nextId := nextId + 1;
				name := n;
				return self;
			)
		post nextId>id;
		
end Exhibitor

class Entity
	instance variables
		protected id: nat1;
		
		protected name: seq of char;
		inv len name <= 50;
		
	operations
		--Returns the id of an entity	
		public pure getId : () ==> nat1
		getId() == (return id);
		
		--Returns the name of an entity
		public pure getName : () ==> seq of char
		getName() == (return name);
		
end Entity